plans:
  - name: test-empty-interpolation-spherical
    actions:
      - type: interpolate
        outputSize: 1639680
        inputConfiguration:
          setByValue:
            gridType: sh
            spectral: true
            truncation: 1279
        jobConfiguration:
          empty:
            #setByValue:
            #  gridType: reduced_gg
            #  gridded: true
        outputConfiguration:
          empty:
      - type: encode
        format: grib
        template: @CMAKE_BINARY_DIR@/multio/tests/interpolate/data/spherical-PL/mars-spherical-orig.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false
            path: @CMAKE_BINARY_DIR@/multio/tests/interpolate/sanityCheckEmptyInterpolation/multio-spherical-orig.grib
