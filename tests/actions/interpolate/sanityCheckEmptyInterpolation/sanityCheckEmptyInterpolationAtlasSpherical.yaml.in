plans:
  - name: test-empty-interpolation-spherical
    actions:
      - type: interpolate
        from: SH1279
        to: SH1279
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/interpolate/data/spherical-PL/mars-spherical-orig.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false
            path: @multio_BINARY_DIR@/tests/interpolate/sanityCheckEmptyInterpolation/multio-spherical-orig.grib
