plans:
  - name: test-interpolate-reducedgg-050x050
    actions:
      - type: select
        match:
          - param: 133
      - type: interpolate
        from: O1280
        to: L720x361
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/actions/interpolate/data/reducedgg/MarsFullFrameReducedgg050x050.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false # Will give you one file per server
            path: @multio_BINARY_DIR@/tests/actions/interpolate/full-frame/atlas/MultioFullFrameReducedgg050x050.grib
