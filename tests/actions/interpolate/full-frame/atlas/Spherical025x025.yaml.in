plans:
  - name: test-interpolate-spectral-025x025
    actions:
      - type: select
        match:
          - param: 129
      - type: interpolate
        from: TCO1279
        to: L1440x721
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/actions/interpolate/data/spherical/MarsFullFrameSpherical025x025.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false # Will give you one file per server
            path: @multio_BINARY_DIR@/tests/actions/interpolate/full-frame/atlas/MultioFullFrameSpherical025x025.grib
