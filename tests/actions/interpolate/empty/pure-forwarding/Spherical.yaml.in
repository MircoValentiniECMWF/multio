plans:
  - name: test-empty-interpolation-spherical
    actions:
      - type: interpolate
        outputSize: 1639680
        inputConfiguration:
          setByValue:
            gridType: sh
            spectral: true
            truncation: 1279
        jobConfiguration:
          empty:
            #setByValue:
            #  gridType: reduced_gg
            #  gridded: true
        outputConfiguration:
          empty:
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/actions/interpolate/data/spherical/MarsSphericalOrig.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false
            path: @multio_BINARY_DIR@/tests/actions/interpolate/empty/pure-forwarding/MultioSphericalOrig.grib
