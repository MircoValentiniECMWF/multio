plans:
  - name: test-empty-interpolation-reducedGG
    actions:
      - type: interpolate
        outputSize: 6599680
        inputConfiguration:
          loadFromFile:
            pl: @multio_BINARY_DIR@/tests/actions/interpolate/data/PlO1280.dat
          setByValue:
            gridType: reduced_gg
            gridded: true
            north: 90.0
            south: -90.0
            west: 0.0
            east: 360.0
            N: 1280
        jobConfiguration:
          empty:
        outputConfiguration:
          empty:
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/actions/interpolate/data/reducedgg/MarsReducedggOrig.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false
            path: @multio_BINARY_DIR@/tests/actions/interpolate/empty/pure-forwarding/MultioReducedggOrig.grib
