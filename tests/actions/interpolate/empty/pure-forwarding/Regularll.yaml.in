plans:
  - name: test-empty-interpolation-regularLL
    actions:
      - type: interpolate
        outputSize: 65160
        inputConfiguration:
          setByValue:
            gridType: regular_ll
            gridded: true
            north: 90.0
            south: -90.0
            west: 0.0
            east: 360.0
            south_north_increment: 1.0
            west_east_increment: 1.0
            Ni: 360
            Nj: 181
        jobConfiguration:
          empty:
            #setByValue:
            #  gridType: reduced_gg
            #  gridded: true
        outputConfiguration:
          empty:
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/actions/interpolate/data/regularll/MarsRegularllOrig.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false
            path: @multio_BINARY_DIR@/tests/actions/interpolate/empty/pure-forwarding/MultioRegularllOrig.grib
