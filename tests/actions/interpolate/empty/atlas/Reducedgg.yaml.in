plans:
  - name: test-empty-interpolation-reducedGG
    actions:
      - type: interpolate
        from: O1280
        to: O1280
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/actions/interpolate/data/reducedgg/MarsReducedggOrig.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false
            path: @multio_BINARY_DIR@/tests/actions/interpolate/empty/atlas/MultioReducedggOrig.grib
