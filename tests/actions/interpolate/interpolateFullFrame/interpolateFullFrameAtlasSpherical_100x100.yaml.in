plans:
  - name: test-interpolate-spectral-100x100
    actions:
      - type: select
        match:
          - param: 129
      - type: interpolate
        from: SH1279
        to: L360x181
      - type: encode
        format: grib
        template: @multio_BINARY_DIR@/tests/interpolate/data/spherical-PL/mars-spherical-100x100.grib
      - type: sink
        sinks:
          - type: file
            append: false
            per-server: false # Will give you one file per server
            path: @multio_BINARY_DIR@/tests/interpolate/interpolateFullFrame/multio-spherical-100x100.grib
